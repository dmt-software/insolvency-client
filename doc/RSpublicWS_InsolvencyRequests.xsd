	<xs:simpleType name="persoonGeboortedatum">
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="inspubAdresPostcode">
		<xs:restriction base="xs:string">
			<xs:length value="6"/>
			<xs:pattern value="[1-9][0-9]{3}[A-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="inspubAdresHuisnummer">
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<xs:simpleType name="inspubHandelsnaam">
		<xs:restriction base="xs:string">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KvKNummer">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{5,8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="inspubPublicatiekenmerk">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{2}\.){0,1}[a-z]{3}\.[0-9]{2}\.[0-9]{1,4}\.[F|S|R]\.[0-9]{4}\.[0-9]{1,2}\.[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="inspubInsolventienummer">
		<xs:restriction base="xs:string">
			<xs:pattern value="[F|S|R]\.(\d{2}/){0,1}\d{2}/\d{1,4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="getCase">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="publicationNumber" type="inspubPublicatiekenmerk"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchByDate">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="date" type="xs:dateTime"/>
				<xs:element name="court" type="instantieRechtbankCode"/>
				<xs:element name="pubType" minOccurs="0" maxOccurs="7">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Uitspraken faillissement">
								<xs:annotation>
									<xs:documentation xml:lang="en">Rulings on the commencement of bankruptcy procedures</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Uitspraken schuldsanering">
								<xs:annotation>
									<xs:documentation xml:lang="en">Rulings on the commencement of procedures on debt restucturing</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Uitspraken surseance">
								<xs:annotation>
									<xs:documentation xml:lang="en">Rulings on the commencement of procedures on moratorium</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Einde faillissementen">
								<xs:annotation>
									<xs:documentation xml:lang="en">Rulings on the termination of bankruptcy procedures</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Einde schuldsaneringen">
								<xs:annotation>
									<xs:documentation xml:lang="en">Rulings on the termination of procedures on  debt restucturing</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Einde surseances">
								<xs:annotation>
									<xs:documentation xml:lang="en">Rulings on the termination of moratorium</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Overig">
								<xs:annotation>
									<xs:documentation xml:lang="en">Other insolvency publications</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchInsolvencyID">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="insolvencyID" type="inspubInsolventienummer"/>
				<xs:element name="court" type="instantieRechtbankCode" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchModifiedSince">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="modifyDate" type="xs:dateTime"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchNaturalPerson">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element name="dateOfBirth" type="persoonGeboortedatum"/>
					<xs:choice>
						<xs:sequence>
							<xs:element name="prefix" type="inspubPersoonVoorvoegsel" minOccurs="0"/>
							<xs:element name="surname" type="inspubPersoonAchternaam"/>
						</xs:sequence>
						<xs:sequence>
							<xs:element name="houseNumber" type="inspubAdresHuisnummer"/>
							<xs:element name="postalCode" type="inspubAdresPostcode"/>
						</xs:sequence>
					</xs:choice>
				</xs:sequence>
				<xs:sequence>
					<xs:sequence>
						<xs:element name="prefix" type="inspubPersoonVoorvoegsel" minOccurs="0"/>
						<xs:element name="surname" type="inspubPersoonAchternaam"/>
					</xs:sequence>
					<xs:sequence>
						<xs:element name="houseNumber" type="inspubAdresHuisnummer"/>
						<xs:element name="postalCode" type="inspubAdresPostcode"/>
					</xs:sequence>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="searchUndertaking">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:choice>
						<xs:element name="name" type="inspubHandelsnaam"/>
						<xs:element name="commercialRegisterID" type="KvKNummer"/>
					</xs:choice>
					<xs:element name="postalCode" type="inspubAdresPostcode" minOccurs="0"/>
					<xs:element name="houseNumber" type="inspubAdresHuisnummer" minOccurs="0"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="name" type="inspubHandelsnaam"/>
					<xs:element name="commercialRegisterID" type="KvKNummer"/>
					<xs:element name="postalCode" type="inspubAdresPostcode" minOccurs="0"/>
					<xs:element name="houseNumber" type="inspubAdresHuisnummer" minOccurs="0"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="postalCode" type="inspubAdresPostcode"/>
					<xs:element name="houseNumber" type="inspubAdresHuisnummer"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
